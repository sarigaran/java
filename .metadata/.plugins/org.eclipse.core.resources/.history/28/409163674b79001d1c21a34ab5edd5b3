package obj.java;

public class Circle {

		 int r=10;
		double area(){	
			double area=3.14 *r*r;
			return area;
		}
		double perimeter() {	
			double quo=2*3.14*r;
			return quo;
		}
public class ex{
	public static void main(String[] args) {
			
	Circle obj=new Circle();
	System.out.println(obj.area());
	System.out.println(obj.perimeter());
		}
	}
}


