package org.java;


import java.io.Serializable;
public class Student implements Serializable{    
       	
			
			int id;
			String dop;
			float score;
			public int getId() {
				return id;
			}
		
			public void setId(int id) {
				this.id = id;
			}
			public String getDop() {
				return dop;
			}
			public void setDop(String dop) {
				this.dop = dop;
			}
			public float getScore() {
				return score;
			}
			public void setScore(float score) {
				this.score = score;
			}

			public Student(int id, String dop, float score) {
				super();
				this.id = id;
				this.dop = dop;
				this.score = score;
			}
			
}