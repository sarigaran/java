package j6_part2;

public class Script {

	public static void main(String[] args) {
		int num=5;
		int sum=0;
		int a=0;
		int i=1;
		while (i <=num) {
			a=i;
			System.out.println("number"+a);
			sum=sum+i;
			i++;
			
		
		System.out.println("    total number is "+sum);
		int rev=0;a=234567;
		System.out.println();
		while(a!=0)
		{
			
			int rem=a%10;
			rev=rev*10+rem;
			a=a/10;
		}	
		System.out.println("reverse number  "+rev);
		}}
}
