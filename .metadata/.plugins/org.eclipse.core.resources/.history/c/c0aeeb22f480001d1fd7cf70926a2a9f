package j6_part2;

public class Script {

	public static void main(String[] args) {
		int num=5;
		int sum=0;
		int a=0;
		for (int i = 1; i <=num; i++) {
			a=i;
			System.out.println("number"+a);
			sum=sum+i;
		}
		System.out.println("    total number is "+sum);
		int rev=0;
		System.out.println();
		while(a!=0)
		{
			
			int rem=a%10;
			rev=rev*10+rem;
			a=a/10;
		}	
		System.out.println("reverse number  "+a);
		}
}
