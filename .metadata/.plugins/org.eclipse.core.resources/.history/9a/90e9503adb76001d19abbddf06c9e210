package j7_part2;

public class Dimaond {

		
		
			public static void main(String[] args) {
			

				int i, k, count = 1;
      
        count = 5 - 1;
        for (k = 1; k <=5 ; k++)
        {
            for (i = 1; i <= count; i++)
                System.out.print(" ");
            count--;
            for (i = 1; i <= 2 * k - 1; i++)
              System.out.print("*");
            System.out.println();
        }
        count = 1;
        for (k = 1; k <= 5 - 1; k++)
        {
            for (i = 1; i <= count; i++)
              System.out.print(" ");
            count++;
            for (i = 1; i <= 2 * (5 - k) - 1; i++)
              System.out.print("*");
            System.out.println();
        }
  }
}